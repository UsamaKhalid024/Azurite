<aura:component implements="forceCommunity:availableForAllPageTypes,lightning:isUrlAddressable,force:hasRecordId"  access="global" >
    
    
    
    
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="overrideAttrib" type="String" default="" />
    <aura:attribute name="invoiceBody" type="String"  />
    
    <aura:handler name="init" value="{!this}" action="{!c.init}" />
    
    <!--<input type="text" id="fahad" value="here there" />
    <lightning:button value="click me" label=" click me" onclick="{!c.hideButton}" />-->
    
    <ui:outputRichText aura:id="outputRT" value="{!v.invoiceBody}" />
    
    <iframe src="{!v.overrideAttrib}" id="invoice-iframe" width="100%" height="1147px" frameBorder="0" scrolling="no" />
    <aura:html tag="style">
    </aura:html>
    
</aura:component>